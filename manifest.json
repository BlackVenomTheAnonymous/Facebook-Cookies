{
  "name": "Facebook Bot",
  "version": "1.5",
  "description": "Import/export Cookies which can be used to let bot login.",
  "permissions": [
    "cookies",
    "activeTab",
    "https://*.facebook.com/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "logo/16.jpg",
      "32": "logo/32.jpg",
      "48": "logo/48.jpg",
      "64": "logo/64.jpg",
      "128": "logo/128.jpg"
    }
  },
  "icons": {
    "16": "logo/16.jpg",
    "32": "logo/32.jpg",
    "48": "logo/48.jpg",
    "64": "logo/64.jpg",
    "128": "logo/128.jpg"
  },
  "manifest_version": 2
}
